
@{
    ViewBag.Title = "Cập nhật bảng chấm công";
}

<h1 style="font-family: 'Times New Roman'; color:darkblue">Cập nhật bảng chấm công</h1>
<br />
@using (Html.BeginForm("Update", "ImportBangChamCong", FormMethod.Post, new { enctype = "multipart/form-data" }))
